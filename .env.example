# This file contains the default environment variables for the application.
# You can override these variables by creating a .env file in the root of the project.

# PostgreSQL
# The name of the database to use.
POSTGRES_DB=nokari
# The username to use for the database.
POSTGRES_USER=nokari
# The password to use for the database.
POSTGRES_PASSWORD=password

# Web App
# The port that the web app will run on.
WEB_APP_PORT=3000

# Backend
# The port that the backend will run on.
BACKEND_PORT=8000
LOG_PATH=./logs

# Google API
# The API key to use for the Google API.
GOOGLE_API_KEY=
# The ID of the custom search engine to use.
CUSTOM_SEARCH_ENGINE_ID=

# Firebase
# The JSON object containing the Firebase service account credentials.
FIREBASE_CREDENTIALS_JSON=

# Frontend Firebase Configuration
# Get these values from your Firebase Console (Project Settings -> General -> Your apps)
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=
VITE_FIREBASE_MEASUREMENT_ID=
